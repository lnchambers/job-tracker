<main class="main-container">
  <section class="main-left"></section>
  <section class="main-side">
    <section class="main-side-container">
      <section class="main-side-top"></section>
      <nav class="nav">
        <%= link_to "Companies", companies_path %>
        <%= link_to "Jobs", jobs_path %>
        <%= link_to "Categories", categories_path %>
        <%= link_to "Dashboard", dashboard_path %>
        <%= link_to "Tags", tags_path %>
      </nav>
      <section class="main-side-bottom">
        <h1>All Tags</h1>
        <section class="companies">
          <% @tags.each do |tag| %>
            <div class="company-card">
              <%= link_to tag.title, tag_path(tag) %>
              <%= link_to "Edit", edit_tag_path(tag) %>
              <%= link_to "Delete", tag_path(tag), method: "delete" %>
            </div>
          <% end %>
      </section>
    </section>
  </section>
</main>
