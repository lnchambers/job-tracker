All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul><%= job.title %> at <%= job.company.name %></ul>
<% end %>

<ul>
  <% @jobs.each do |job| %>
  <li class=job_<%= job.id %>>
    <%= link_to job.title, job_path(job) %>:
    <%= link_to "Edit", edit_job_path(job) %>
    <%= link_to "Delete", job_path(job), method: "delete" %>
  </li>
  <% end %>
</ul>
