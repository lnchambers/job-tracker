All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul><%= job.title %> at <%= job.company.name %></ul>
<% end %>

<ul>
  <% @jobs.each do |job| %>
  <li class=job_<%= job.id %>>
    <%= link_to job.title, company_jobs_path %>:
    <%= link_to "Edit", edit_company_job_path %>
    <%= link_to "Delete", company_jobs_path, method: "delete" %>
  </li>
  <% end %>
</ul>
